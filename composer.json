{
    "name": "ins/media-api",
    "type": "project",
    "description": "Example for integration of Sonata MediaBundle with ApiPlatform 2.0 including media upload and exposing media formats in GET item operation",
    "require": {
        "php": ">=7.0",
        "symfony/symfony": "3.1.*",
        "cweagans/composer-patches": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Ins\\MediaApiBundle\\": ""
        }
    },
    "extra": {
        "patches": {
            "sonata-project/media-bundle": {
                "Move CategoryInterface": "vendor/ins/media-api/Resources/patches/media-bundle.patch"
            }
        }
    }
}
